digraph {
    fake [style = invisible]
    "{1, 2}" [root = true, shape = doublecircle]
    fake -> "{1, 2}" [style = bold]
    "{1, 3}" [shape = circle]
    "{2, 3}" [shape = doublecircle]
    "∅" [shape = circle];
    "{1, 2}" -> "{1, 3}" [label="a"];
    "{1, 2}" -> "∅" [label="b"];
    "{1, 3}" -> "{2, 3}" [label="a,b"];
    "{2, 3}" -> "{1, 2}" [label="a"];
    "{2, 3}" -> "{2, 3}" [label="b"];
    "∅" -> "∅" [label="a,b"];
}
